/*
 With commment-json, our json files can have JS-style comments.
 Here is a leading block-style comment.
*/
{
  "mock-config": {  /* another block comment */
    "bool-value": true,
    "int-value": 17,
    "float-value": /*block in the middle here */ 3.14159,
    "string-value": "Lorem Ipsum",
    "null-value": null, // end of line comment
    // another comment
    "array-value": [ true, 17, 3.14159, "Lorem Ipsum", null ],
    "map-value": {
      "bool-value": false,
      "int-value": 19,
      "float-value": 2.71828,
      "string-value": "Non-nummy num",
      "null-value": null
    },
    "source": "config/unit-testing.json"
  }
}
// /* here is a comment inside a comment */
/*
// comment inside comment
// /* more comments?
*/
// final comment
